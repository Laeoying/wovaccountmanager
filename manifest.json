{
    "manifest_version": 3,
    "name": "Wolvesville Account Manager",
    "version": "1.0",
    "description": "Manage and change your account with a few clicks in Wolvesville.",
    "author": "Iris",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.wolvesville.com/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}